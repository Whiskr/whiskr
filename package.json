{
  "name": "whiskr",
  "version": "1.0.0",
  "description": "A Tinder-like progressive web application for pairing local adoptable pets with potential owners",
  "main": "secrets.js",
  "scripts": {
    "test": "mocha",
    "start": "NODE_ENV=production node server",
    "seed": "node ./seed",
    "build": "npm run build-css && cd client && yarn build",
    "build-css": "node-sass-chokidar client/src/styles -o client/src/",
    "watch-css": "concurrently \"npm run build-css \" \" node-sass-chokidar client/src/styles -o client/src/ --watch --recursive\"",
    "sync": "concurrently \"yarn \" \"cd client && yarn \"",
    "dev": "concurrently \"nodemon server\" \"cd client && yarn start\" ",
    "deploy": "scripts/deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Whiskr/whiskr.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Whiskr/whiskr/issues"
  },
  "homepage": "https://github.com/Whiskr/whiskr#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "connect-session-sequelize": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "http-status-cats": "^1.0.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "^0.0.3",
    "nodemailer": "^4.4.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "pg": "6.4.1",
    "prop-types": "^15.6.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-fontawesome": "^1.6.1",
    "react-modal": "^3.1.5",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "react-swipe-card": "^0.1.4",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass": "^1.0.0-beta.3",
    "sequelize": "^4.22.6",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0"
  }
}
